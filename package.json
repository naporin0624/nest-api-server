{
  "name": "nest-api-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild:rm": "rimraf dist",
    "server:build:prod": "webpack --config webpack/server/webpack.config.prod.js",
    "client:build:prod": "webpack --config webpack/client/webpack.config.prod.js",
    "prebuild:build": "run-p client:build:prod server:build:prod",
    "build": "run-s prebuild:rm prebuild:build",
    "start:prod": "node dist/server.js",
    "start:dev": "webpack --config webpack/server/webpack.config.dev.js",
    "lint:server": "eslint server --ext .ts",
    "lint:client": "eslint client --ext .ts,.tsx",
    "lint:fix:server": "eslint server --ext .ts --fix",
    "lint:fix:client": "eslint client --ext .ts,.tsx --fix",
    "lint:fix": "run-p lint:fix:client lint:fix:server",
    "api:build": "aspida --build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js --config ormconfig.dev.js",
    "entity:create": "yarn typeorm entity:create -n",
    "migration:create": "yarn typeorm migration:create -n",
    "migration:generate": "yarn typeorm migration:generate -n",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "migration:show": "yarn typeorm migration:show",
    "migration:prod": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js --config ormconfig.prod.js migration:run"
  },
  "dependencies": {
    "@aspida/axios": "^0.1.1",
    "@grpc/proto-loader": "^0.5.1",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/microservices": "^6.5.3",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/platform-express": "^6.10.1",
    "@nestjs/platform-socket.io": "^6.9.0",
    "@nestjs/serve-static": "^1.0.1",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.2.0",
    "@nestjs/websockets": "^6.9.0",
    "cache-manager": "^2.10.1",
    "axios": "^0.19.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.0",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "date-fns": "^2.4.1",
    "google-protobuf": "^3.9.0",
    "grpc": "^1.23.2",
    "json2csv": "^4.5.4",
    "mongo": "^0.1.0",
    "mongoose": "^5.7.5",
    "mysql": "^2.17.1",
    "nest-schedule": "^0.6.4",
    "path": "^0.12.7",
    "protobufjs": "^6.11.3",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "recharts": "^1.8.5",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rx": "^4.1.0",
    "rxjs": "^6.3.3",
    "socket.io-client": "^2.3.0",
    "source-map-loader": "^0.2.4",
    "styled-components": "^4.4.1",
    "swagger-ui-express": "^4.0.7",
    "typeorm": "latest",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@nestjs/testing": "6.1.1",
    "@types/axios": "^0.14.0",
    "@types/compression": "^1.0.1",
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/express": "4.16.1",
    "@types/jest": "^24.0.18",
    "@types/json2csv": "^4.5.1",
    "@types/mongoose": "^5.5.9",
    "@types/node": "^12.12.11",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/recharts": "^1.8.3",
    "@types/rx": "^4.1.1",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/styled-components": "^4.4.0",
    "@types/supertest": "2.0.7",
    "@types/webpack": "^4.41.0",
    "@types/webpack-dev-middleware": "^2.0.3",
    "@types/webpack-hot-middleware": "^2.16.5",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-polyfill": "^6.26.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.16.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "24.7.1",
    "merge": "^1.2.1",
    "nodemon": "^1.19.3",
    "nodemon-webpack-plugin": "^4.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "supertest": "4.0.2",
    "ts-essentials": "^3.0.4",
    "ts-jest": "24.0.2",
    "ts-loader": "^6.2.0",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "typescript": "3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.9.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@/(.+)": "<rootDir>/$1",
      "^static/(.*)$": "<rootDir>/../static/$1"
    },
    "rootDir": "server",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}